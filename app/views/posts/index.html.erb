
  <div class="col-xs-12 title">
      SG Blog
  </div>

  <div class="col-xs-2   posts ">
    <table  class="table table-striped">
      <% @posts.each do |post| %>
        <tr >
          <td >
            <a class="post-title" href="<%= post_path(post.id)%>" >  <%=post.title%></a>
         </td>
            <% end %>
        </tr>
        <tr>
          <td class="new-post-link">
            <a class=" link" id="New Post" href="<%= new_post_path%>" role="button">New Post</a>
          </td>
        </tr>
    </table>
  </div>
  <div class="col-xs-10 x">
  </div>
