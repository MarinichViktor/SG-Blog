<!DOCTYPE html>
<html>
<head>
  <title>SgBlog</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% flash.each do |k, v| %>
  <div id="<%= k %>"><%= v %></div>
<% end %>
<%= render :partial => "layouts/header"%>


<%= yield %>


<%= render :partial => "layouts/footer"%>

</body>
</html>
